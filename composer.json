{
  "name": "sabrihamda/google-translator-bundle",
  "type": "symfony-bundle",
  "description": "Symfony Translator using google",
  "keywords": [],
  "homepage": "https://github.com/SabriHamda/SH-Google-Translator-Bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "sabri Hamda",
      "email": "sabri@hamda.ch"
    }
  ],
  "require": {
    "php": "^7.1.1",
    "symfony/http-kernel": "^4.2",
    "symfony/translation": "^4.2",
    "symfony/finder": "^4.2",
    "twig/twig": "^2.7",
    "symfony/twig-bundle": "^4.2",
    "symfony/console": "^4.2"
  },
  "require-dev": {
    "symfony/dependency-injection": "~3.4|~4.0",
    "phpunit/phpunit": "^6.5"
  },
  "conflict": {
    "symfony/dependency-injection": "<3.4",
    "symfony/event-dispatcher": "<3.4",
    "symfony/var-dumper": "<3.4"
  },
  "autoload": {
    "psr-4": {
      "GoogleTranslatorBundle\\": "/"
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    }
  }
}
